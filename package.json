{
  "name": "electronchat",
  "version": "1.0.0",
  "description": "An electron test app",
  "main": "build/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "npm-run-all clean compile build:* webpack:build",
    "build:assets": "node -e \"require('fs').cpSync('./static/assets', './build/assets', {recursive: true});\"",
    "clean": "npx rimraf build",
    "compile": "tsc",
    "dist": "npm-run-all clean compile build:* webpack:dist",
    "lint": "eslint ./src --ext \".js,.jsx,.ts,.tsx\"",
    "lint:fix": "eslint ./src --ext \".js,.jsx,.ts,.tsx\" --fix",
    "package": "electron-forge package",
    "prestart": "npm run compile",
    "start": "electron-forge start",
    "webpack:watch": "webpack --watch --mode development --no-devtool",
    "webpack:build": "webpack --mode development --no-devtool",
    "webpack:dist": "webpack --mode production --config webpack.prod.js",
    "make": "electron-forge make"
  },
  "keywords": [
    "electron"
  ],
  "author": "Lorenzo Franceschini",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@electron-forge/cli": "^7.3.1",
    "@electron-forge/maker-deb": "^7.3.1",
    "@electron-forge/maker-rpm": "^7.3.1",
    "@electron-forge/maker-squirrel": "^7.3.1",
    "@electron-forge/maker-zip": "^7.3.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@electron-forge/plugin-fuses": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-loader": "^9.1.3",
    "electron": "^29.1.6",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
